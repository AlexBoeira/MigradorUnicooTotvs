      584              rowErrors.errorNumber      = pErrorNumber
      585              rowErrors.errorType        = "GP"
      586              rowErrors.errorSubType     = "INFORMATION"
      587              rowErrors.errorDescription = errorDescription
      588              rowErrors.errorParameters  = pErrorParameters.
