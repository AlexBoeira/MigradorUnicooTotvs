      599       else assign rowErrors.errorHelp = ErrorHelp.
      600   
      601       if rowErrors.errorHelp <> ""
      602       then rowErrors.errorHelp = rowErrors.errorHelp + chr(10).
      603   
      604       rowErrors.errorHelp = rowErrors.errorHelp + rowErrors.errorParameters.
