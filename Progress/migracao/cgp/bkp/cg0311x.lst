     2527               where teadgrpa.cd-modalidade      = b-import-propost.cd-modalidade
     2528                 and teadgrpa.nr-proposta        = nr-proposta-aux
     2529                 and teadgrpa.cd-grau-parentesco = import-faixa-propost.cd-grau-parentesco
