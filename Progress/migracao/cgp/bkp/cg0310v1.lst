     2813                                          RUN pi-cria-tt-erros("Dt Incl Plano origem: "   +
     2814                                                               if import-bnfciar.dt-inclusao-origem <> ?
     2815                                                               then string(import-bnfciar.dt-inclusao-origem,"99/99/9999")
